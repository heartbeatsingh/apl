<div class="row">
        
        <!-- NEW COL START -->
        <article class="col-sm-12 col-md-12 col-lg-6">
                    
            <!-- Widget ID (each widget will need unique ID)-->
            <div class="jarviswidget" id="wid-id-3" data-widget-colorbutton="false" data-widget-editbutton="false" data-widget-custombutton="false">
                <!-- widget options:
                usage: <div class="jarviswidget" id="wid-id-0" data-widget-editbutton="false">
    
                data-widget-colorbutton="false"
                data-widget-editbutton="false"
                data-widget-togglebutton="false"
                data-widget-deletebutton="false"
                data-widget-fullscreenbutton="false"
                data-widget-custombutton="false"
                data-widget-collapsed="true"
                data-widget-sortable="false"
    
                -->
                <header>
                    <span class="widget-icon"> <i class="fa fa-edit"></i> </span>
                    <h2>Auctions List </h2>
    
                </header>
    
                <!-- widget div-->
                <div>
    
                    <!-- widget edit box -->
                    <div class="jarviswidget-editbox">
                        <!-- This area used as dropdown edit box -->
    
                    </div>
                    <!-- end widget edit box -->
    
                    <!-- widget content -->
                    <div class="widget-body no-padding">
                            <table id="dt_basic" class="table table-striped table-bordered table-hover" width="100%">
                                    <thead>			                
                                        <tr>
                                            <th>#</th>
                                            <th>Name</th>
                                            <th>Email</th>                                       
                                            <th>Basic Value</th>
                                            <th>Auction Time</th>
                                            <th>Auction Status</th>
                                           
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% results.forEach(function(row){   %>
                                           
                                            <tr>
                                                <td>
                                                    
                                                <% if(row.player.picture){ %>
                                                    <img src="/players/<%= row.player.picture %>" alt="<%= row.name %>" title="<%= row.name %>" class="" width="80">
                                                <% }else{ %>
                                                    <img src="/img/avatars/male.png" alt="<%= row.name %>" title="<%= row.name %>" class="" width="80">
                                                <% } %>
                                                </td>
                                                <td> 
                                                   <%= row.player.name %>
                                                </td>
                                                <td><%= row.player.email %></td>                                            
                                                <td><%= row.player.basic_player_value %></td>
                                                <td><%= row.time_limit %></td>
                                                
                                                <td>
                                                    <% if(row.status){ %>
                                                        <a href="javascript:void(0);" class="btn btn-default btn-circle" title="Activate"><i class="glyphicon glyphicon-ok"></i></a>
                                                    <% }else{ %>
                                                        <a href="javascript:void(0);" class="btn btn-default btn-circle" title="InActivate"><i class="glyphicon glyphicon-remove"></i></a>
                                                    <% } %>
                                                </td>
                                         
                                            </tr>     
                                        <%
                                    });
   
                                        %>                              
                                    </tbody>
                                </table>
                        
    
                    </div>
                    <!-- end widget content -->
    
                </div>
                <!-- end widget div -->
    
            </div>
            <!-- end widget -->
    
        </article>
        <!-- END COL -->
    
        <!-- NEW COL START -->
        <article class="col-sm-12 col-md-12 col-lg-6">
    
            <!-- Widget ID (each widget will need unique ID)-->
            <div class="jarviswidget" id="wid-id-4" data-widget-colorbutton="false" data-widget-editbutton="false" data-widget-custombutton="false">
                <!-- widget options:
                usage: <div class="jarviswidget" id="wid-id-0" data-widget-editbutton="false">
    
                data-widget-colorbutton="false"
                data-widget-editbutton="false"
                data-widget-togglebutton="false"
                data-widget-deletebutton="false"
                data-widget-fullscreenbutton="false"
                data-widget-custombutton="false"
                data-widget-collapsed="true"
                data-widget-sortable="false"
    
                -->
                <header>
                    <span class="widget-icon"> <i class="fa fa-edit"></i> </span>
                    <h2>About Auction </h2>
    
                </header>
    
                <!-- widget div-->
                <div>
    
                    <!-- widget edit box -->
                    <div class="jarviswidget-editbox">
                        <!-- This area used as dropdown edit box -->
    
                    </div>
                    <!-- end widget edit box -->
    
                    <!-- widget content -->
                    <div class="widget-body no-padding">
                        <div class="col-sm-12 col-md-12 col-lg-12">
				
							<div class="well">
				
								<h3>Hello Teams <small>Need your attention!</small></h3>
				
								<div class="jumbotron">
									<h1 id="timer">Hello, world!</h1>
									<p>
                                        Few minuts are left for start auction, wish you all the best and goodluck to choose better players for your team. <br> Cheers!! <br>
                                    <a href="/auctions/dashboard">Here you can set your bids for each player, when they will list there one by one.</a>
									</p>
								</div>				
							</div>
				
						</div>
                   
    
                    </div>
                    <!-- end widget content -->
                </div>
                <!-- end widget div -->
            </div>
            <!-- end widget -->
        </article>
        <!-- END COL -->        
    </div>

    <script type="text/javascript">
        var timeoutHandle;
        function countdown(minutes) {
            var seconds = 60;
            var mins = minutes
            function tick() {
                var counter = document.getElementById("timer");
                var current_minutes = mins-1
                seconds--;
                counter.innerHTML =
                current_minutes.toString() + ":" + (seconds < 10 ? "0" : "") + String(seconds);
                if( seconds > 0 ) {
                    timeoutHandle=setTimeout(tick, 1000);
                } else {
                    $("#auctionOuter").html('<span class="txt-color-red"><strong>Auction time up</strong></span>');
                    /*$.bigBox({
                            title : "Opps!",
                            content : "The bidding time has been closed for {playerName}. Maximum bidding price will decide, {playerName} will play in whom team.",
                            color : "#D30A03",
                            //timeout: 8000,
                            icon : "fa fa-bell swing animated",
                            number : "1"
                    });*/
                    if(mins > 1){
                       // countdown(mins-1);   never reach “00″ issue solved:Contributed by Victor Streithorst
                       setTimeout(function () { countdown(mins - 1); }, 1000);    
                    }
                }
            }
            tick();
        }
        countdown(60);
        </script>